{
  "name": "ua-parser-ts",
  "version": "0.0.1",
  "description": "Angular2 user agent parser",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings -- install",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --profile --colors --display-error-details --display-cached",
    "server": "webpack-dev-server --config ./config/webpack.default.js --inline --colors --progress --display-error-details --display-cached --port 4000  --content-base src",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "test": "karma start",
    "ci": "npm run lint && npm test && npm run e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server",
    "preversion": "npm test",
    "version": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "~0.16.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "html-webpack-plugin": "^2.21.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "to-string-loader": "^1.1.4",
    "typescript": "~1.8.9",
    "typings": "^1.3.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/http": "2.0.0-rc.4",
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "angular2-text-mask": "^0.10.1",
    "core-js": "^2.2.0",
    "moment": "^2.13.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  }
}
